SELECT id, username, email, created_at 
FROM users 
WHERE is_active = true 
ORDER BY created_at DESC; пользователи 


SELECT id, name, frequency, target_count 
FROM habits 
WHERE user_id = 1 AND is_active = true 
ORDER BY name; привычки 

SELECT date, weight, body_fat_percent 
FROM weight_tracking 
WHERE user_id = 1 
ORDER BY date DESC 
LIMIT 10; вес

SELECT date, value, notes 
FROM goal_progress 
WHERE goal_id = 1 
ORDER BY date DESC; цели 